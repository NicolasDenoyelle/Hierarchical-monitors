	OBJ:=Core;
	PERF_LIB:=hierarchical;
	EVSET:=PU;
	EVSET_REDUCE:=$0/$1;
	SAMPLES_REDUCE:=MONITOR_SAMPLES_LAST;
	N_SAMPLES:=4;
	SILENT:=0;
	MAX:=0;
	MIN:=0;
}

